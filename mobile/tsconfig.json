{
  "compilerOptions": {
    "target": "ES2020",
    "lib": ["ES2020", "DOM", "ESNext.Intl"],
    "module": "ESNext",
    "jsx": "react-jsx",
    "moduleResolution": "Bundler",
    "strict": true,
    "allowJs": true,
    "skipLibCheck": true,
    "noEmit": true,
    "baseUrl": ".",
    "paths": {
      "@/*": ["./src/*"]
    }
  },
  "include": ["App.tsx", "index.js", "src/**/*"]
}
